{
  "name": "medical-reporting-platform",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise-grade Medical Reporting Platform with Next.js 14 and Prisma",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "db:generate": "cd apps/web && npx prisma generate",
    "db:push": "cd apps/web && npx prisma db push",
    "db:seed": "cd apps/web && npx prisma db seed",
    "db:studio": "cd apps/web && npx prisma studio"
  },
  "dependencies": {
    "turbo": "^2.0.0"
  },
  "devDependencies": {
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "npm@9.0.0"
}
